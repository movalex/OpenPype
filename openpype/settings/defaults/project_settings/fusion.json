{
    "imageio": {
        "activate_host_color_management": true,
        "ocio_config": {
            "override_global_config": false,
            "filepath": []
        },
        "file_rules": {
            "activate_host_rules": false,
            "rules": {}
        }
    },
    "masterprefs": "{\nLocked = true,\nGlobal = {\n  Paths = {\n    Map = {\n      [\"OpenPype:\"] = \"$(OPENPYPE_FUSION)/deploy\",\n      [\"Config:\"] = \"UserPaths:Config;OpenPype:Config\",\n      [\"Scripts:\"] = \"UserPaths:Scripts;Reactor:System/Scripts;OpenPype:Scripts\",\n    },\n  },\n  Script = {\n    PythonVersion = 3,\n    Python3Forced = true\n    },\n  UserInterface = {\n    Language = \"en_US\"\n    },\n  },\n}",
    "copy_fusion_settings": {
        "copy_path": "~/.openpype/hosts/fusion/profiles",
        "copy_status": false,
        "force_sync": false
    },
    "create": {
        "CreateSaver": {
            "temp_rendering_path_template": "{workdir}/renders/fusion/{subset}/{subset}.{frame}.{ext}",
            "default_variants": [
                "Main",
                "Mask"
            ],
            "instance_attributes": [
                "reviewable",
                "farm_rendering"
            ]
        }
    },
    "templated_workfile_build": {
        "profiles": [
            {
                "task_types": [
                    "Compositing"
                ],
                "task_names": [],
                "path": "{root[work]}/{project[name]}/templates/comp.comp",
                "keep_placeholder": true,
                "create_first_version": true
            }
        ]
    }
}
